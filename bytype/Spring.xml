<?xml version="1.0" encoding= "UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="s" class="bytype.student" autowire="byType">
		<property name="s_id" value="102"></property>
		<property name="s_name" value="rutu"></property>
	</bean>

	<bean id="addrs" class="bytype.addrs">
		<property name="area_Name" value="Wagholi"></property>
		<property name="city_Name" value="pune"></property>
	</bean>

	<bean id="m" class="bytype.Mobile">
		<property name="no" value="123456789"></property>
	</bean>
</beans>